<div class="breadcrumbs-light pb-0 pt-4" id="breadcrumbs-wrapper">
  <!-- Search for small screen-->
  <div class="container">
    <div class="row">
      <div class="col s10 m6 l6">
        <h5 class="breadcrumbs-title mt-0 mb-0"><span><%= t('activerecord.models.customer') %></span></h5>
        <ol class="breadcrumbs mb-0">
          <li class="breadcrumb-item"><a href="/"><%= t('home') %></a>
          </li>
          <li class="breadcrumb-item"><a href="#"><%= t('bpm.process') %></a>
          </li>
          <li class="breadcrumb-item active"><%= t('breadcrumb.edit') %>
          </li>
        </ol>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <% @list.each do |process| %>
    <div class="col s12 m6">
      <div class="card-panel">
        <h4 class="header2"><%= process[:name] || process[:key]%> <small><%= process[:key] %></small></h4>
        <%= process[:description] %>
        <%= link_to "Start Process", bpm_start_process_path(process[:key]), class: :btn %>
        <%= link_to "Edit Process", bpmn_editor_path(process[:key]), class: :btn %>
      </div>
    </div>

  <% end %>
</div>

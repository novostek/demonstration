<div class="col s12">
  <div class="container">
      <div class="section">

          <div class="row">
              <div class="col s12 mb-0 pb-0 animate fadeLeft right-align">
                  <div class="row">
                      <div class="col s12">
                          <p class="medium-small pb-0 mb-0">30 days summary</p>
                      </div>
                  </div>
              </div>
              <col class="s12">
              <div class="row">
                  <div class="col s12">
                      <div class="col s12 m4">
                          <div class="dashboard-chart card pt-0 pb-0 animated fadeInLeft border-radius-5">
                              <div class="dashboard-income-wrapper padding-2 ml-2">
                                  <span class="badge indigo gradient-shadow mt-2 mr-2">+ <%= number_to_currency(@day_incomes, :unit => '$ ') %> today</span>
                                  <p class="mt-2 mb-0 font-weight-600">Income <%= @income %></p>
                                  <h5>
                                    <%= number_to_currency(@year_incomes.map { |m| m[:value] }.reduce(0, :+), :unit => '$ ')%>
                                  </h5>
                              </div>
                              <div class="chart-wrapper">
                                  <canvas id="income-dashboard-chart" class="center"></canvas>
                              </div>
                          </div>
                      </div>
                      <div class="col s12 m4">
                          <div class="dashboard-chart card pt-0 pb-0 animated fadeInLeft fast border-radius-5">
                              <div class="dashboard-income-wrapper padding-2 ml-2">
                                  <span class="badge red gradient-shadow mt-2 mr-2">- <%= number_to_currency(@day_costs, :unit => '$ ') %> today</span>
                                  <p class="mt-2 mb-0 font-weight-600">Expenses</p>
                                  <h5>
                                    <%= number_to_currency(@year_costs.map { |m| m[:value] }.reduce(0, :+), :unit => '$ ')%>
                                  </h5>
                              </div>
                              <div class="chart-wrapper">
                                  <canvas id="expenses-dashboard-chart" class="center"></canvas>
                              </div>
                          </div>
                      </div>
                      <div class="col s12 m4">
                          <div class="dashboard-chart card pt-0 pb-0 animated fadeInLeft faster border-radius-5">
                              <div class="dashboard-income-wrapper padding-2 ml-2">
                                  <p class="mt-2 mb-0 font-weight-600">Balance</p>
                                  <h5><%= number_to_currency(@total_balance[:value], :unit => '$ ') %></h5>
                              </div>
                              <div class="chart-wrapper">
                                  <canvas id="balance-dashboard-chart" class="center"></canvas>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
              <div class="col s12 m8 mt-0 pt-0">
                  <div class="row">
                      <div class="col s12 m6">
                          <div class="card z-depth-0  border-radius-5 transparent ">
                              <div class="card-content p-0">
                                  <div class="row  animated fadeInDown faster" style="animation-delay: .7s;">
                                      <div class="col s12">
                                          <span class="left width-100 left-align indigo-text font-weight-900">Receivables
                                          </span>
                                          <span class="left width-100 display-flex left-align">
                                            <%= number_to_currency(@receivables, :unit => '$') %>
                                          </span>
                                      </div>
                                  </div>
                                  <div class="row mt-2 animated fadeInDown fast" style="animation-delay: .7s;">
                                      <div class="col s12">
                                          <span class="left width-100 left-align indigo-text font-weight-900">Overdue orders</span>
                                          <span class="left width-100 display-flex left-align">
                                            <%= number_to_currency(@overdue, :unit => '$') %>
                                          </span>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>

                      <div class="col s12 m6">
                          <div class="card z-depth-0 border-radius-5 transparent">
                              <div class="card-content p-0">
                                  <div class="row animated fadeInDown faster" style="animation-delay: .7s;">
                                      <div class="col s12">
                                          <span class="left width-100 left-align red-text font-weight-900">Materials costs</span>
                                          <span class="left width-100 display-flex left-align">
                                            <%= number_to_currency(@material_costs, :unit => '$') %>
                                          </span>
                                      </div>
                                  </div>


                                  <div class="row mt-2 animated fadeInDown fast" style="animation-delay: .7s;">
                                      <div class="col s12">
                                          <span class="left width-100 left-align red-text font-weight-900">Workers costs</span>
                                          <span class="left width-100 display-flex left-align">
                                            <%= number_to_currency(@labor_costs, :unit => '$') %>
                                          </span>
                                      </div>
                                  </div>




                              </div>
                          </div>
                      </div>

                  </div>
                  <div class="divider mb-2" style="margin-top: 30px;"></div>
                  <h6 class="mb-0 mt-0 display-flex justify-content-between">Orders
                      <a href="#" class="waves-effect"></a>
                  </h6>
                  <div class="row">

                      <div class="col s4">
                          <div class="card orders-card animated fadeInUp faster border-radius-8 ">
                              <div class="card-content pl-7 pr-7">
                                  <div class="row">
                                      <div class="col s4 pl-3">
                                          <i class="material-icons transparent indigo-text background-round mt-5">new_releases</i>
                                      </div>
                                      <div class="col s8">
                                          <span class="font-weight-600">New</span><br>
                                          <h5 class="mt-0"><a class="indigo-text" href="#"><%= @new_orders %></a></h5>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                      <div class="col s4">
                          <div class="card orders-card animated fadeInUp fast border-radius-8 ">
                              <div class="card-content pl-7 pr-7">
                                  <div class="row">
                                      <div class="col s4 pl-3">
                                          <i class="material-icons transparent green-text background-round mt-5">done</i>
                                      </div>
                                      <div class="col s8">
                                          <span class="font-weight-600">Finished</span><br>
                                          <h5 class="mt-0"><a class="green-text" href="#"><%= @finished_orders %></a></h5>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                      <div class="col s4">
                          <div class="card orders-card animated fadeInUp border-radius-8">
                              <div class="card-content pl-7 pr-7">
                                  <div class="row">
                                      <div class="col s4 pl-3">
                                          <i class="material-icons transparent background-round mt-5">clear</i>
                                      </div>
                                      <div class="col s8">
                                          <span class="font-weight-600">Cancelled</span><br>
                                          <h5 class="mt-0"><a class="grey-text" href="#"><%= @cancelled_orders %></a></h5>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>
                  <div class="card recent-orders-card animate fadeLeft border-radius-5">
                      <div class="card-content">


                          <h6 class="mb-0 mt-0 display-flex justify-content-between mb-5">Latest
                              orders
                              <a href="#" class="waves-effect"><i class="material-icons float-right">more_vert</i></a>
                          </h6>
                          <ul class="collection">
                            <% @latest_orders.each do |order| %>
                              <li class="collection-item">
                                  <p class="font-weight-600 display-flex justify-content-between">
                                    <%= order.get_customer_name %> <span><%= number_to_currency(order.get_order_value) %></span> </p>
                                  <p class="medium-small">
                                    <%= link_to order.code, order_path(order) %>
                                  </p>

                              </li>
                            <% end %>
                          </ul>

                      </div>
                  </div>
              </div>
              <div class="col s12 m4">

                  <div class="card padding-4 animate fadeRight border-radius-5">
                      <div class="row">
                          <div class="col s5 m5">
                              <h5 class="mb-0"><%= @count_new_customers %></h5>
                              <p class="no-margin">New</p>
                              <p class="mb-0 pt-8"><%= @total_customers %></p>
                          </div>
                          <div class="col s7 m7 right-align">
                              <i class="material-icons background-round mt-5 mb-5 woffice-color gradient-shadow black-text">perm_identity</i>
                              <p class="mb-0">Total Clients</p>
                          </div>
                      </div>
                  </div>

                  <div class="card recent-clients-card animate fadeRight border-radius-5">
                      <div class="card-content">
                          <h4 class="card-title mb-0">Recent clients <a href="#"><i class="material-icons float-right">more_vert</i></a></h4>
                          <ul class="collection mb-0">
                            <% @recent_customers.each do |customer| %>
                              <li class="collection-item">
                                <p class="font-weight-600">
                                  <%= link_to customer.name, customer_path(customer) %>
                                </p>
                                <p class="medium-small"><%= customer.created_at.strftime("%d, %B %Y") %></p>
                              </li>
                            <% end %>                              
                          </ul>
                      </div>
                  </div>
                <div class="card animate fadeRight">
                  <div class="card-content">
                    <h4 class="card-title mb-0">Conversion Ratio</h4>
                    <div class="conversion-ration-container mt-8">
                      <div id="conversion-ration-bar-chart" class="conversion-ration-shadow"></div>
                    </div>
                    <p class="medium-small center-align">This month conversion ratio</p>
                    <h5 class="center-align mb-0 mt-0"><%= @conversion_rate %>%</h5>
                  </div>
                </div>
              </div>
          </div>
      </div>


<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
<!-- BEGIN: Head-->

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
  <meta name="description" content="Woffice is a powerful business management tool.">
  <meta name="keywords" content="woffice management square crm erp">
  <meta name="author" content="ThemeSelect">
  <title><%= "#{ yield :title }  Woffice " %></title>
  <link rel="apple-touch-icon" href="/materialize/images/favicon/apple-touch-icon-152x152.png">
  <%= favicon_link_tag '/favicon.png' %>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!-- BEGIN: VENDOR CSS-->
  <link rel="stylesheet" type="text/css" href="/materialize/vendors/vendors.min.css">
  <link rel="stylesheet" type="text/css" href="/materialize/vendors/flag-icon/css/flag-icon.min.css">
  <link rel="stylesheet" href="/materialize/vendors/select2/select2.min.css" type="text/css">
  <link rel="stylesheet" href="/materialize/vendors/select2/select2-materialize.css" type="text/css">
  <link rel="stylesheet" type="text/css" href="/materialize/vendors/data-tables/css/jquery.dataTables.min.css">
  <link rel="stylesheet" type="text/css" href="/materialize/vendors/data-tables/extensions/responsive/css/responsive.dataTables.min.css">
  <link rel="stylesheet" type="text/css" href="/materialize/vendors/data-tables/css/dataTables.checkboxes.css">
  <!-- END: VENDOR CSS-->
  <!-- BEGIN: Page Level CSS-->
  <link rel="stylesheet" type="text/css" href="/materialize/css/themes/vertical-modern-menu-template/materialize.css">
  <link rel="stylesheet" type="text/css" href="/materialize/css/style.min.css">
  <link rel="stylesheet" type="text/css" href="/materialize/css/pages/app-sidebar.css">
  <link rel="stylesheet" type="text/css" href="/materialize/css/pages/app-todo.css">
  <link rel="stylesheet" type="text/css" href="/materialize/css/pages/app-invoice.css">
  <link rel="stylesheet" type="text/css" href="/materialize/css/pages/app-email.css">
  <link rel="stylesheet" type="text/css" href="/materialize/vendors/sweetalert/sweetalert.css">
  <link href="/js/farbtastic/farbtastic.css" rel="stylesheet" type="text/css">
  <link rel="stylesheet" href="/materialize/vendors/chartist-js/chartist.min.css">
  <link rel="stylesheet" href="/materialize/vendors/chartist-js/chartist-plugin-tooltip.css">
  <link href="/css/search_google.css" rel="stylesheet" type="text/css">
  <%# <link href="/css/woffice-style.css" rel="stylesheet" type="text/css"> %>
  <%= stylesheet_link_tag 'materialize-stepper.min' %>
  <%= stylesheet_pack_tag 'application' %>
  <%= stylesheet_link_tag 'app-calendar' %>

  <!-- END: Page Level CSS-->
  <!-- BEGIN: Custom CSS-->
  <link rel="stylesheet" type="text/css" href="/materialize/css/custom/custom.css">
  <!-- END: Custom CSS-->
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'application', media: 'all' %> <!-- , 'data-turbolinks-track': 'reload' -->
  <%= javascript_pack_tag 'application' %> <!-- , 'data-turbolinks-track': 'reload' -->
  <%= yield :head_content %>


  <style>
    @media print{
      html, body{
        visibility: visible !important;
      }
    }
  </style>
</head>
<!-- END: Head-->

<body class="vertical-layout vertical-menu-collapsible page-header-dark vertical-modern-menu preload-transitions 2-columns   <%= yield :body_class %>" data-open="click" data-menu="vertical-modern-menu" data-col="2-columns">

<!-- BEGIN: Header-->
<%= render 'layouts/header' %>
<!-- End Header -->


<!-- BEGIN: SideNav-->
<%= render 'layouts/navside' %>
<!-- END: SideNav-->

<!-- BEGIN: Page Main-->
<div id="main">
  <div class="row">
    <div class="content-wrapper-before woffice-color"></div>
    <div class="col s12">
      <div class="container">
       <%= yield %>
        <div style="height: 70px"></div>
      </div>
    </div>
  </div>
</div>
<!-- END: Page Main-->
<%= yield :sidenav %>
<!-- BEGIN: Footer-->

<footer class="page-footer footer footer-static footer-light woffice-color gradient-shadow navbar-border navbar-shadow">
  <div class="footer-copyright">
    <div class="container"><span>&copy; 2020 <a href="http://www.novostek.com" target="_blank">Woffice LLC</a> All rights reserved.</span>
      <span class="right">Powered by <a href="https://woffice.app/"><img src="/woffice.svg" alt="Woffice"></a></span>
    </div>
  </div>
</footer>

<!-- END: Footer-->
<script src="/materialize/js/vendors.min.js"></script>
<!-- BEGIN VENDOR JS-->
<!-- BEGIN PAGE VENDOR JS-->
<script src="/materialize/vendors/sortable/jquery-sortable-min.js"></script>
<script src="/materialize/vendors/select2/select2.full.min.js"></script>
<%# <script src="/materialize/vendors/quill/quill.min.js"></script> %>
<!-- END PAGE VENDOR JS-->
<!-- BEGIN THEME  JS-->
<script src="/materialize/js/plugins.js"></script>
<script src="/materialize/js/search.js"></script>
<script src="/materialize/js/custom/custom-script.js"></script>
<script src="/materialize/js/custom/materializedatetimepicker.js"></script>
<!-- END THEME  JS-->
<!-- BEGIN PAGE LEVEL JS-->
<!--<script src="/materialize/js/scripts/app-todo.js"></script>-->
<!--<script src="/materialize/js/scripts/app-email.js"></script>-->
<script src="/materialize/vendors/sweetalert/sweetalert.min.js"></script>
<script src="/materialize/js/scripts/extra-components-sweetalert.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.16/jquery.mask.js"></script>
<script data-turbolinks-eval="false" type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAuo1c-9BiXODXuM5L3n26CRdukjlnlUpU&libraries=places"></script>
<script src="/js/farbtastic/farbtastic.js"></script>
<script src="/materialize/js/scripts/advance-ui-toasts.min.js"></script>

<!-- cocoon não está funcionando com o rails 6 por isso importar o JS -->
<script src="/js/cocoon.js"></script>
<script src="/materialize/vendors/chartjs/chart.min.js" type="text/javascript"></script>
<script src="/materialize/vendors/chartist-js/chartist.min.js" type="text/javascript"></script>
<script src="/materialize/vendors/chartist-js/chartist-plugin-tooltip.min.js" type="text/javascript"></script>
<script src="/materialize/vendors/chartist-js/chartist-plugin-fill-donut.min.js" type="text/javascript"></script>


<script type="text/javascript" charset="utf-8">
  $(document).ready(function(){
     $("select").formSelect();
      $('.modal').modal();
      $('tabs').tabs();
  }).on('cocoon:after-insert', function () {

      $('select').formSelect();
      $("select").focusin();
      $('.modal').modal();
  });
  $(document).ready(function(){
      $('.datepicker').datepicker({
        autoClose: true
      });
  });
  $(document).ready(function(){
    $('.phone').mask('(000) 000-0000');
  });

  $(document).ready(function(){
      $("a:contains('Unlicensed copy of the Froala Editor. Use it legally by purchasing a license.')").hide();
  });

  
</script>

<%= toastr_flash %>
<!-- END PAGE LEVEL JS-->
<%= yield :scripts %>
<!-- END PAGE LEVEL JS-->

</body>
</html>
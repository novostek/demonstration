    function fill_array(elem) {
      var months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"]
      var today = new Date();
      var d;
      var month;
      var year;

      for (var i = 0; i <= 12; i++) {
        d = new Date(today.getFullYear(), today.getMonth() - i, 1);
        month = months[d.getMonth()];
        year = d.getFullYear();

        if(!elem.some(value => value.month === month)){
            // month && console.log(month, elem.some(value => String(value.month) === String(month)))
            month && elem.unshift({
            month: month,
            value: 0
            })
        }
      }

      return elem
    }


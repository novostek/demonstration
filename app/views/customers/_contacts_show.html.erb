<div class="row">
  <% contacts.group_by(&:category).map do |category, contatos| %>
      <% contatos.each do |doc| %>
          <div class="col s12 m4">
            <div class="card" style="overflow: hidden;">
              <div class="card-move-up waves-effect waves-block waves-light">
                <div class="move-up cyan darken-1">
                  <div>
                    <span class="chart-title white-text"><%= t doc.category_text %></span>

                  </div>
                  <!--<div class="revenue-line-chart-wrapper"><div class="chartjs-size-monitor"><div class="chartjs-size-monitor-expand"><div class=""></div></div><div class="chartjs-size-monitor-shrink"><div class=""></div></div></div><canvas id="revenue-line-chart" height="138" width="595" class="chartjs-render-monitor" style="display: block; width: 595px; height: 138px;"></canvas></div>-->
                </div>
              </div>
              <div class="card-content">
                <a class="btn-floating btn-move-up waves-effect waves-light red accent-2 z-depth-4 right">
                  <i class="material-icons activator">filter_list</i>
                </a>
                <h6><%= doc.title %></h6>
                <div class="col s12 m5 l6">
                  <div class="trending-bar-chart-wrapper"><div class="chartjs-size-monitor"><div class="chartjs-size-monitor-expand"><div class=""></div></div><div class="chartjs-size-monitor-shrink"><div class=""></div></div></div><canvas id="trending-bar-chart" height="82" width="274" class="chartjs-render-monitor" style="display: block; width: 274px; height: 82px;"></canvas></div>
                </div>
              </div>
              <div class="card-reveal" style="display: none; transform: translateY(0%);">
               <span class="card-title grey-text text-darken-4"><%= doc.title %> <i class="material-icons right">close</i>
               </span>
                <table class="responsive-table">
                  <tbody>
                  <tr>
                    <td><%= t "#{'activerecord.attributes.contact.category'}:" %></td>
                    <td><%= doc.category_text %></td>
                  </tr>
                  <tr>
                    <td><%= t "#{'activerecord.attributes.contact.title'}:" %></td>
                    <td><%= doc.title %></td>
                  </tr>
                  <% if doc.category.address? %>
                      <tr>
                        <td><%= t "#{'json_data.address'}:" %></td>
                        <td><%= doc.data["address"] %></td>
                      </tr>
                      <tr>
                        <td><%= t "#{'json_data.city'}:" %></td>
                        <td><%= doc.data["city"] %></td>
                      </tr>
                      <tr>
                        <td><%= t "#{'json_data.state'}:" %></td>
                        <td><%= doc.data["state"] %></td>
                      </tr>
                      <tr>
                        <td><%= t "#{'json_data.zipcode'}:" %></td>
                        <td><%= doc.data["zipcode"] %></td>
                      </tr>
                      <tr>
                        <td><%= t "#{'json_data.lat'}:" %></td>
                        <td><%= doc.data["lat"] %></td>
                      </tr>
                      <tr>
                        <td><%= t "#{'json_data.lng'}:" %></td>
                        <td><%= doc.data["lng"] %></td>
                      </tr>
                  <% end %>
                  <% if doc.category.phone? %>
                      <tr>
                        <td><%= t "#{'json_data.ddd'}:" %></td>
                        <td><%= doc.data["ddd"] %></td>
                      </tr>
                      <tr>
                        <td><%= t "#{'json_data.phone'}:" %></td>
                        <td><%= doc.data["phone"] %></td>
                      </tr>
                  <% end %>
                  <% if doc.category.email? %>
                      <tr>
                        <td><%= t "#{'json_data.email'}:" %></td>
                        <td><%= doc.data["email"] %></td>
                      </tr>

                  <% end %>
                  </tbody>
                </table>
              </div>
            </div>

          </div>
      <% end %>

  <% end %>
</div>
<style>
  .pac-container {
    z-index: 1051 !important;
  }
</style>

<div class="breadcrumbs-light pb-0 pt-4" id="breadcrumbs-wrapper">
  <!-- Search for small screen-->
  <div class="container">
    <div class="row">
      <div class="col s10 m6 l6">
        <h5 class="breadcrumbs-title mt-0 mb-0"><span><%= t('activerecord.models.account') %></span></h5>
        <ol class="breadcrumbs mb-0">
          <li class="breadcrumb-item"><a href="/"><%= t('home') %></a>
          </li>
          <li class="breadcrumb-item"><a href="#"><%= t('activerecord.models.account') %></a>
          </li>
          <li class="breadcrumb-item active"><%= t('breadcrumb.show') %>
          </li>
        </ol>
      </div>
      <div class="col s2 m6 l6">
        <%= link_to t('button.all'), accounts_path, class:"btn waves-effect waves-light breadcrumbs-btn right" %>
        <%= link_to edit_account_path(@account) , class: "btn-flat waves-effect waves-light breadcrumbs-btn right" do %>
            <i class="material-icons">edit</i>
        <% end %>
      </div>
    </div>
  </div>
</div>

<div class="card-panel">





  <p>
    <strong><%= t('activerecord.attributes.account.name') %>:</strong>
    <%= @account.name %>
  </p>

  <p>
    <strong><%= t('activerecord.attributes.account.type') %>:</strong>
    <%= @account.type %>
  </p>


  <p>
    <strong><%= t('activerecord.attributes.account.category') %>:</strong>
    <%= TransactionCategory.find(@account.transaction_category_id) %>
  </p>


</div>

